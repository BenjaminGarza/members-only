<!DOCTYPE html>
<html>
  <head>
    <title>MembersOnly</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if flash.any? %>
      <ul>
        <% flash.each do |type,msg| %>
          <li> <%= msg %> </li>
        <% end %>
      </ul>
    <% end %>
    <%= yield %>
  </body>
</html>
